{
    "name": "INIAD++",
    "version": "1.2.1",
    "manifest_version": 3,
    "description": "This is a Google Chrome extension that adds various functions to INIAD MOOCs",
    "icons":{
        "16": "img/iniadpp16.png",
        "48": "img/iniadpp48.png",
        "128": "img/iniadpp128.png",
        "512": "img/iniadpp512.png"
    },
    "content_scripts":[{
        "matches":[
            "https://moocs.iniad.org/*",
            "https://docs.google.com/presentation/*",
            "https://www.ace.toyo.ac.jp/ct/home*"
        ],
        "js":[
            "lib/jquery-3.6.0.min.js",
            "lib/jquery-1.12.0.ui.min.js",
            "lib/rgbcolor.js",
            "js/download-button.js",
            "js/download.js",
            "js/colorchange.js",
            "js/backgroundchange.js",
            "js/background.js",
            "js/ace_json.js",
            "js/ace_json_button.js",
            "js/json_upload.js",
            "js/json_timetable_localstorage.js",
            "js/json_timetable.js",
            "js/tools.js",
            "js/moocsmemo_btn.js",
            "js/scrolltop.js",
            "js/drive_buttons.js",
            "js/class_sort_button.js",
            "js/class_sort_default.js",
            "js/class_sort.js",
            "js/class_sort_reflect.js",
            "js/textarea_count.js",
            "js/header.js",
            "js/memolist.js",
            "js/slideoperate.js",
            "js/slide_question.js",
            "js/memonulldelete.js"
        ],
        "css":[
            "lib/jquery-1.12.1.ui.css",
            "css/download-button.css",
            "css/ace_dl.css",
            "css/json_upload.css",
            "css/mymemo.css",
            "css/scrolltop.css",
            "css/class_sort.css",
            "css/settingform.css",
            "css/slideoperate.css",
            "css/pointer.css",
            "css/dragover.css"
        ]
    }]
}
